void main()
{
    int var1=1, var2;
    if(var1==0)
    {
        int a;
    }
    else if(var2==1 && var1==1)
    {
        int b;
    }
    else if(var1==2 || var2==2)
    {
        int c;
    }
    else if(var1==3 && var2==3)
    {
        int d;
    }
    else if(var2==4 || var1==4)
    {
        int e;
    }
    else if(!(var1==5))
    {
        int f;
    }
    else if(var2==6 && var1==6 || var1==5)
    {
        int g;
    }
    else if(var2==6 || var1==7 && var1==5)
    {
        int h;
    }
    else
    {
        int d;
    }
}